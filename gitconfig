[format]
	pretty = "%H %ci %ce %ae %d %s"
[include]
	path = ~/.gitaliases
[push]
	default = current
[fetch]
	prune = true
[diff]
	colorMoved = default
[core]
	pager = delta 
	excludesfile = $HOME/.gitignore_global
[interactive]
	diffFilter = delta --color-only
[delta]
	theme = OneHalfDark
	line-numbers = true
